# this is for album thing
# ScrollView
<Scroller@ScrollView>:
    bar_color: (0,0,0,0)
    bar_inactive_color: (0,0,0,0)
    do_scroll_y:False
    size_hint_y: None
    height: 220

# BoxLayout
<Box_album@BoxLayout>:
    orientation: 'horizontal'
    size_hint_x:None
    width:1320
    spacing:15
    padding:16

# Inter in MDCard
<ICard_album@MDCard>:
    orientation: 'vertical'
    size_hint:None,None
    size:150,130
    radius:[20,]

#Outer in Card
<OCard_album@MDCard>    
    orientation: 'vertical'
    size_hint:None,None
    size:150,180
    radius:[20,] 

# FitImage
<Fit_album@FitImage>:
    radius:[20,]
    pos_hint: {"center_x": .5, "center_y": .5}

# what torrow you can do ,frist give all id to every okk---=======++++++==
 
<HeartBut@MDIconButton>:
    icon:"heart-circle"
<HandBut@MDIconButton>:
    icon:"hand-heart"

<BookBut@MDIconButton>:
    icon:"bookmark"  

