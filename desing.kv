#sound for class
<Sound@MDCard>:
    orientation: 'vertical'
    size_hint: None, None
    size:137,145
    elevation:10
    radius:[30,]

<Information@Label>:
    markup:True
    font_size:18
    text_size:root.width,None
    height:self.texture_size[1]
    size_hint_y:None
    padding_x:15

<Spec_Food@MDCard>:
    orientation: 'vertical'
    size_hint:None,None
    width:150
    height:180

<Sound_icon@MDBoxLayout>
    orientation: 'horizontal'
    adaptive_height: True       
        
    MDIconButton:
        icon:"volume-source"
    MDIconButton:
        icon:"restart"  

<Audio_icon@MDIconButton>
    icon:"stop-circle"
    on_release:
        if self.icon == "stop-circle":self.icon="pause-circle"
        else :self.icon="stop-circle"

